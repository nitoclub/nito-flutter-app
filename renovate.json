{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":timezone(Asia/Tokyo)",
    ":automergeMinor",
    "group:allNonMajor"
  ],
  "dependencyDashboard": true,
  "labels": [
    "dependencies"
  ],
  "schedule": [
    "before 7am"
  ],
  "automergeSchedule": [
    "before 7am"
  ],
  "commitMessagePrefix": ":wrench:",
  "pruneBranchAfterAutomerge": true
}
